                {% endif %}
            </div> #}
            <div class="navbar-collapse collapse" id="navbarSupportedContent"> 
                <ul class="navbar-nav mr-auto">

                    {{ content.wpNavMenu|raw }}
                </ul>
            </div><!--/.nav-collapse -->
        
            
</nav> <!-- end navbar-->


{% if content.navbarFixed == 'default' %}
      </header>
    </div> <!-- /container -->
{% endif %}


