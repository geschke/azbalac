<!DOCTYPE html>
<html {{ content.languageAttributes|raw }}>
    <head>
      <meta charset="{{ content.bloginfo.charset }}" />
      <meta name="description" content="{{ content.bloginfo.description }}">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="profile" href="http://gmpg.org/xfn/11">
	<link rel="pingback" href="{{ content.bloginfo.pingback_url }}">

    {{ content.headerImageData|raw }}

    {% if content.navbarFixed == 'fixed-top' and content.hasNavMenuHeaderMenu %}
            <style type="text/css">
                body {
                    padding-top: 70px !important;
                }
                body.admin-bar .navbar-fixed-top { top: 28px !important; }

                /* v4 notice above main navbar */
                .v4-tease {
                display: block;
                padding: 15px 20px;
                font-weight: bold;
                color: #fff;
                text-align: center;
                background-color: #0275d8;
                }
                .v4-tease:hover {
                color: #fff;
                text-decoration: none;
                background-color: #0269c2;
                }

                
            </style>
            {% endif %}

            {{ content.subfooterStyles|raw }}

        {{ content.wpHead|raw }}    

        {{ content.sectionFontStyles|raw }}

        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

</head>


    <body {{ content.bodyClass|raw }}>
    <div id="skip-link"><a href="#content" class="sr-only element-focusable">{{ content.skipToMainContent }}</a></div>

 {% if content.navbarFixed == 'default' %}

      
    <div id="header" role="banner" style="background-color: {{ content.headerStyles.headerStyleColorBg }} {{ content.headerStyles.headerStyleColorFg }}">

<div class="container">
    
    <div class="masthead col-md-12 col-sm-12">
        <h1 id="site-header-text"><a class="header-url" style="{{ content.headerStyles.headerStyleColorFg  }}" href="{{ content.homeUrl }}">{{ content.description }}</a></h1>
        <h2 id="site-description">{{ content.subtitleDescription }}</h2>
         <div id="site-header">
                <a href="{{ content.homeUrl }}" rel="home">
                    <img id="site-header-image" src="{{ content.headerImage }}" width="1" height="1" data-width="{{ content.customHeader.width }}" data-height="{{ content.customHeader.height }}" alt="{{ content.headerImageAlt }}">
                </a>
            </div>
    </div>
 </div>
{% endif %}

{% if content.isFrontPage %}
    {{ content.showSlider_1|raw }}

    {{ content.introductionElements.1|raw }}
{% endif %}

{% if content.navbarFixed == 'default' %}
    <div class="container">
{% endif %}

{% if content.hasNavMenu.headerMenu %}

    <!-- Fixed navbar -->
    <div class="navbar {{ content.headerStyles.navbarStyleClass }}" role="navigation">
        {% if content.navbarFixed == 'fixed-top' %}
            <div class="container">
        {% endif %}   
        <div id="navbar-header" class="navbar-header">
                <div id="navbar-toggle-screenreader"><a href="#" class="sr-only element-focusable" data-toggle="collapse" data-target=".navbar-collapse">{{ content.toggleNavigation }}</a></div>

                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">{{ content.toggleNavigation }}</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                {% if content.navbarFixed == 'fixed-top' %}
                    <div id="site-header-text"><a class="navbar-brand" href="{{ content.homeUrl }}">{{ content.description }}</a></div>
                {% endif %}
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    {{ content.wpNavMenu|raw }}
                </ul>
            </div><!--/.nav-collapse -->

            {% if content.navbarFixed == 'fixed-top' %}
                </div>
            {% endif %}
            
    </div> <!-- end navbar-->

{% else %}
    <p>&nbsp;</p>
{% endif %}

{% if content.navbarFixed == 'default' %}
        </div> <!-- container -->
    </div> <!-- header in default navbar -->
{% endif %}

{% if content.isFrontPage %}
    {{ content.showIntroductionElements.2|raw }}
{% endif %}

      

